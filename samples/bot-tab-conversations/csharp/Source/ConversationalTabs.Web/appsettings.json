{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "ClientId": "<<aad-id>>",
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "<<tenant-id>>",
    "Audience": "<<aad-id>>",
    "ClientSecret": "<<aad-client-secret>>"
  },
  "Bot": {
    "MicrosoftAppType": "MultiTenant",
    "MicrosoftAppId": "<<aad-id>>",
    "MicrosoftAppPassword": "<<aad-client-secret (Obtained from the Azure portal for the bot's registration)>>"
  },
  "Graph": {
    "BaseUrl": "https://graph.microsoft.com/v1.0",
    "Scopes": "https://graph.microsoft.com/.default"
  },
  "HostUrl": "https://<<ngrok-url>>",
  "ExternalTeamsAppId": "<<teams-app-store-app-id>>"
}
