{
    "AzureAd": {
        "Instance": "https://login.microsoftonline.com/",
        "TenantId": "<<YOUR-TENANT-ID>>",
        "ClientId": "<<YOUR-APP-ID>>",
        "AppSecret": "<<YOUR-APP-PASSWORD>>",
        "ApplicationIdURI": "api://<<BASE-URL-DOMAIN>>/<<YOUR-APP-ID>>",
        "AuthUrl": "/oauth2/v2.0/token",
        "ValidIssuers": "https://login.microsoftonline.com/{YOUR-TENANT-ID}/v2.0,https://sts.windows.net/{YOUR-TENANT-ID}/"
    },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}