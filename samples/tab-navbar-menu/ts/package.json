{
  "name": "microsoft-teams-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fluentui/react-northstar": "^0.49.0",
    "@microsoft/teams-js": "^2.35.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.1",
    "typescript": "^3.2.1"
  },
  "scripts": {
    "dev:teamsfx": "env-cmd --silent -f .localConfigs npm run start",
