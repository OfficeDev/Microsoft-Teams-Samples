{
    "AzureAd": {
        "Instance": "https://login.microsoftonline.com/",
        "TenantId": "$TenantId",
        "MicrosoftAppId": "$ClientId",
        "MicrosoftAppPassword": "$ClientSecret",
        "ApplicationIdURI": "api://$DomainName/$ClientId",
        "GraphURI": "https://graph.microsoft.com/",
        "AuthUrl": "/oauth2/v2.0/token",
        "ValidIssuers": "https://login.microsoftonline.com/tenantId/v2.0,https://sts.windows.net/tenantId/",
        "TeamsAppDisplayName":  "$TeamsAppDisplayName",
        "ApplicationUrl": "$BaseUrl",
        "ContainerTypeId": "$ContainerTypeId"
    },
    "AzureAdConsumer": {
        "TenantId": "$ConsumerTenantId"
    },
    "AccessTokenScopes": {
        "Graph": "https://graph.microsoft.com/.default"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
}