@model IEnumerable<Microsoft.BotBuilderSamples.Controllers.DeepLinkChannelModel>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
   <script src="https://statics.teams.cdn.office.net/sdk/v1.7.0/js/MicrosoftTeams.min.js" integrity="sha384-00JbifySIlPvW32u9rSurgu8PujfL6XFdV9iNn4ZWyurJJ33MFvpwPqmCHDq9ADv" crossorigin="anonymous"></script>
    <script>
        microsoftTeams.initialize();         
        microsoftTeams.getContext(function (context) {            
            console.log(context);           
                $('#tableList').show();                
            }
        })
    </script>
    
       <table id="tableList" style="margin-left:50px;margin-top:30px">
   
        @foreach (var item in Model)
        {
        <tr style="font-family:'Segoe UI';font-size:20px">
            <td>
                @Html.ActionLink(item.linkTitle, "DetailChannel", new { id = item.ID })
            </td>           
        </tr>
        <tr></tr>
        <tr></tr>
    }
</table>