{
  "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
  "manifestVersion": "1.16",
  "version": "1.0",
  "id": "845d8001-f01d-4887-b909-2dd673af7bb8",
  "packageName": "com.microsoft.teams.meetings.package",
  "developer": {
    "name": "{DeveloperName}",
    "websiteUrl": "{WebsiteUrl}",
    "privacyUrl": "{BaseUrl}/privacy",
    "termsOfUseUrl": "{BaseUrl}/termsofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Link Unfurling Sample",
    "full": "Link Unfurling Sample"
  },
  "description": {
    "short": "{Short description.}",
    "full": "{Long description.}"
  },
  "showLoadingIndicator": true,
  "bots": [
    {
      "botId": "{Bot App Id}",
      "scopes": [
        "personal",
        "team",
        "groupchat"
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "accentColor": "#FFFFFF",
  "configurableTabs": [
    {
      "configurationUrl": "{BaseUrl}/Config",
      "canUpdateConfiguration": true,
      "scopes": [
        "team",
        "groupchat"
      ],
      "context": [
        "channelTab",
        "meetingChatTab",
        "meetingDetailsTab",
        "meetingSidePanel",
        "meetingStage"
      ]
    }
  ],
  "composeExtensions": [
    {
      "botId": "{Bot App Id}",
      "canUpdateConfiguration": true,
      "messageHandlers": [
        {
          "type": "link",
          "value": {
            "domains": [
              "{{domain-name}}"
            ]
          }
        }
      ],
      "commands": [
        {
          "id": "Search",
          "type": "query",
          "title": "Title",
          "description": "Description",
          "initialRun": false,
          "fetchTask": false,
          "parameters": [
            {
              "name": "Parameter",
              "title": "Title",
              "description": "Description",
              "inputType": "text"
            }
          ]
        }
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "{{domain-name}}",
    "*.botframework.com",
    "*.microsoft.com"
  ],
  "webApplicationInfo": {
    "id": "{Bot App Id}",
    "resource": "api://{domain}/{Bot App Id}"
  }
}