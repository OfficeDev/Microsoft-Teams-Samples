{
	"MicrosoftAppType": "MultiTenant", // Specifies the app type, MultiTenant allows the bot to be used by multiple tenants
	"MicrosoftAppId": "{your-bot-id}", // The unique bot ID from your Azure AD app registration
	"MicrosoftAppPassword": "{current password}", // The password (client secret) for your bot's Azure AD app
	"MicrosoftAppTenantId": "{your-tenant-id}", // The Tenant ID for the bot's Azure AD app
	"AzureOpenAIEndpoint": "{your-azure-openai-endpoint}", // The endpoint URL for your Azure OpenAI instance
	"AzureOpenAIKey": "{your-azure-openai-api-key}", // The API key for accessing Azure OpenAI services
	"AzureOpenAIDeployment": "{your-deployment-name}" // The name of the deployment within Azure OpenAI you want to use
}