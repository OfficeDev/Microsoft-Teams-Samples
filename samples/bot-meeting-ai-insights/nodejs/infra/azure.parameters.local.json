{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceBaseName": {
      "value": "botgraphai${{RESOURCE_SUFFIX}}"
    },
    "botDisplayName": {
      "value": "Bot graph meeting AI insights"
    },
    "aadAppClientId": {
      "value": "${{BOT_ID}}"
    },
    "aadAppClientSecret": {
      "value": "${{SECRET_BOT_PASSWORD}}"
    },
    "microsoftAppTenantId": {
      "value": "${{TEAMS_APP_TENANT_ID}}"
    },
    "botAppDomain": {
      "value": "${{BOT_DOMAIN}}"
    }
  }
}
